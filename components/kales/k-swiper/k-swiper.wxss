.swiper-dots {
  position: absolute;
  top: -40rpx;
  left: 50%;
  transform: translateX(-50%);
}

.dot {
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: rgba(255, 255, 255, 0.6);
  margin-right: 14rpx;
}

.dot:last-child {
  margin-right: 0
}

.dot-active {
  background-color: red;
}

.item {}

.swiper-item-container {
  width: 100%;
  height: 100%;
}