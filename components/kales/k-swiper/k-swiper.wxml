<view class="k-swiper relative border-box custom-class" style="width:{{width}};height:{{height}}">
  <swiper previous-margin="{{pm}}" next-margin="{{nm}}" current="{{current}}" bindchange="onSwiperChange"
    style="height:100%;">
    <swiper-item class="item" wx:for="{{lists}}" wx:key="*this">
      <view class="swiper-item-container border-box" style="{{kstyle}}">
        {{item.content}}
      </view>
    </swiper-item>
  </swiper>
  <slot></slot>
</view>